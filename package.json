{
	"name": "eslint-plugin-node-import",
	"version": "1.1.1",
	"description": "Disallow imports of built-in Node.js modules without the `node:` prefix",
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin"
	],
	"author": "Nikita Karamov <me@kytta.dev>",
	"contributors": [
		"Andrey Sitnik <andrey@sitnik.ru>",
		"Azat S <to@azat.io>",
		"Már Örlygsson <mar.nospam@anomy.net>",
		"Christopher Hiller <boneskull@boneskull.com>"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kytta/eslint-plugin-node-import.git"
	},
	"bugs": "https://github.com/kytta/eslint-plugin-node-import/issues",
	"main": "./lib/index.js",
	"exports": "./lib/index.js",
	"scripts": {
		"lint": "pnpm run /^lint:/",
		"lint:eslint-docs": "pnpm run update:eslint-docs --check",
		"lint:js": "eslint .",
		"test": "mocha tests --recursive",
		"update:eslint-docs": "eslint-doc-generator"
	},
	"files": [
		"lib/"
	],
	"dependencies": {
		"globals": "^17.3.0"
	},
	"devDependencies": {
		"@types/eslint": "^8.56.12",
		"eslint": "^8.57.1",
		"eslint-doc-generator": "^1.7.1",
		"eslint-plugin-eslint-plugin": "^5.5.1",
		"eslint-plugin-n": "^16.6.2",
		"mocha": "10.2.0"
	},
	"engines": {
		"node": "^14.18.0 || ^16.0.0 || >= 18.0.0"
	},
	"peerDependencies": {
		"eslint": ">=7"
	},
